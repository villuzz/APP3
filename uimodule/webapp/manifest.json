{
  "_version": "1.12.0",
  "sap.app": {
    "id": "PM030.APP3",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "mainService": {
        "uri": "/catalog/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "cloud.service"
  },  
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.60",
      "libs": {
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "PM030.APP3.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupId": "$direct"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "PM030.APP3.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
					"pattern": "",
					"name": "default",
					"target": [
						"TilePage"
					]
				},
				{
					"pattern": "TilePage",
					"name": "TilePage",
					"target": [
						"TilePage"
					]
				},  
        {
					"pattern": "ViewPage",
					"name": "ViewPage",
					"target": [
						"ViewPage"
					]
				},       
        {
          "name": "RouteMainView",
          "pattern": "RouteMainView",
          "target": [
            "TargetMainView"
          ]
        },
        {
					"pattern": "WizardPage",
					"name": "WizardPage",
					"target": [
						"WizardPage"
					]
				}
      ],
      "targets": {
        "TargetMainView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "MainView",
          "viewName": "MainView"
        },
        "WizardPage": {
					"controlAggregation": "pages",
					"viewName": "WizardPage",
					"viewId": "WizardPage",
					"viewLevel": 1,
					"transition": "slide"
				},
				"TilePage": {
					"controlAggregation": "pages",
					"viewName": "TilePage",
					"viewId": "TilePage",
					"viewLevel": 1,
					"transition": "slide"
				},
        "ViewPage": {
					"controlAggregation": "pages",
					"viewName": "ViewPage",
					"viewId": "ViewPage",
					"viewLevel": 1,
					"transition": "slide"
				}
      }
    },
    "rootView": {
      "viewName": "PM030.APP3.view.MainView",
      "type": "XML",
      "async": true,
      "id": "MainView"
    }
  }
}
